{
  "name": "mediconnect-backend",
  "version": "1.0.0",
  "description": "MediConnect BD Backend API Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:init": "node scripts/initializeDatabase.js",
    "seed": "node scripts/seeders/masterSeed.js",
    "db:clear": "node scripts/clearDatabase.js",
    "db:refresh": "node scripts/clearDatabase.js && node scripts/seeders/masterSeed.js",
    "db:reset": "node scripts/initializeDatabase.js && node scripts/seeders/masterSeed.js"
  },
  "keywords": [
    "mediconnect",
    "healthcare",
    "api"
  ],
  "author": "MediConnect BD Team",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
